{
    "manifest_version": 2,
    "name": "BG3 Wiki Search",
    "version": "1.1.0",
    "description": "Simple wiki search for Baldur's Gate 3",
    "icons": {
        "96": "icons/icon_96.png",
        "152": "icons/icon_152.png"
    },
    "permissions": [],
    "browser_action": {
        "default_icon": "icons/favicon.png",
        "default_title": "BG3 Wiki Search",
        "default_popup": "ui/ui.html"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Alt+A"
            }
        }
    }
}